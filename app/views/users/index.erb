メンバー一覧画面
<body>
<%= link_to "部屋に戻る", room_path(@room) %>
<div class="game_top">
<% @users.each do |user| %>
	<div class="game_content">
	<div class="game_name"><%= user.nickname %></div>
	
  <div class="game_image"=><%= user.my_image.attached? ? image_tag(user.my_image) : '<画像がありません>'.html_safe %></div>
	<div class="user_career"><%= "ゲーム歴：#{user.career.name}" %></div>
	<div class="user_likes"><%= "好き：#{user.likes}" %></div>
	<div class="user_weakness"><%= "苦手：#{user.weakness}" %></div>
	<div class="user_sns"><%= user.sns.present? ? "SNS:#{user.sns}" : 'SNS:なし' %></div>
	<div class="user_note"><%= raw user.note %></div>
	</div>
	<% end %>

</div>
</body>