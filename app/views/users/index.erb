メンバー一覧画面
<body>
<%= link_to "部屋に戻る", room_path(@room) %>
<div class="game_top">
<% @users.each do |user| %>
  <div class="game_content">
    <a href="<%= room_user_game_path(@room, user, @game) %>">
        <div class="game_name"><%= user.nickname %></div>
        <div class="game_image"> <%= user.my_image.attached? ? image_tag(user.my_image) : image_tag('sample.jpg') %></div>
        <div class="user_career"><%= user.career.present? ? "ゲーム歴：#{user.career.name}" : "ゲーム歴：なし" %></div>
        <div class="user_likes"><%= user.likes.present? ? "好き：#{user.likes}" : "好き：なし" %></div>
        <div class="user_weakness"><%= user.weakness.present? ? "苦手：#{user.weakness}" : "苦手：なし" %></div>        
        <div class="user_sns"><%= user.sns.present? ? "SNS:#{user.sns}" : 'SNS:なし' %></div>
        <div class="user_note"><%= user.note.present? ? raw(user.note) : "なし" %></div>
        </a>
   
  </div>
<% end %>

</div>
</body>